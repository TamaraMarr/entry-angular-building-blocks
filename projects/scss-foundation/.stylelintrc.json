{
    "extends": "@enigmatry/stylelint-config",
    "rules": {
      "value-keyword-case": [
        "lower",
        { "ignoreKeywords": ["largeDesktop", "extraLargeDesktop"] }
      ],
      "scss/no-duplicate-dollar-variables": [
        true,
        { "ignoreInsideAtRules": ["each", "while"] }
      ]
    }
  }